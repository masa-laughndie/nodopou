<% provide(:title, @user.name + "(@" + @user.account_id + ")さんのリスト一覧") %>
<% provide(:og_title, @user.name + "(@" + @user.account_id + ")さんのリスト一覧") %>
<% provide(:og_url, lists_url(@user)) %>

<div class="container content-nspace">
  <div class="title-nspace c">
    <h1><%= @user.name %>さんのリスト一覧</h1>
  </div>
  <div class="border"></div>
  <ul>
    <%= render partial: @lists,
               locals: { user: @user,
                         mode: 'normal' } %>
  </ul>
</div>