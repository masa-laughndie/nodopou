var
  $target = $('#list-check-<%= @mylist.id %>'),
  uncheckButton = $target.find('.button-uncheck');
if ( uncheckButton.length != 0 ) {
  $target.html("<%= escape_javascript(render partial: 'mylists/check', locals: { mylist: @mylist }) %>");
} else {
  $target.html("<%= escape_javascript(render partial: 'mylists/uncheck', locals: { mylist: @mylist }) %>");
}